{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/encrypted/**",
        "destination": "/web_encrypted/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
